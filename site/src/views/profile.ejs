<!DOCTYPE html>
<html lang="en">

<%- include('./partials/head') %>

<body class="profile">

    <%- include('./partials/header') %>

      <section class="usuario">
        <div class="container">
            <div class="perfil">
                <img src="/images/usuarios/<%= usuario.image%>"> <br>
                <small><%= usuario.DNI %> <br> <%= usuario.Sexo %></small>
                <h1><%= usuario.Nombre %> <%= usuario.Apellido %></h1>
                <h2><%= usuario.email %></h2>
            </div>
        </div>
    </section>

    <%- include('./partials/footer') %>
</body>

</html>